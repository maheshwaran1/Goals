# Uncomment the next line to define a global platform for your project
 platform :ios, '15.0'

target 'Goals' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!


  # Pods for Goals


  # Realm Swift
   pod 'RxSwift'
   pod 'RxCocoa'
   pod 'RxDataSources'

 # Realm
   pod 'RealmSwift'

 # Google Sign
   #pod 'GoogleSignIn', '~>5.0'
   pod 'GoogleSignIn'

 # Firebase
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'

# spinner
  pod 'JGProgressHUD'

#Image
  pod 'SDWebImage'


end
